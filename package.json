{
  "name": "auth-api",
  "version": "1.0.0",
  "description": "Authentication API with Express, Prisma, MongoDB and JWT",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest --runInBand --detectOpenHandles",
    "test:watch": "jest --watch",
    "prisma:generate": "prisma generate --schema=prisma/user.schema.prisma",
    "prisma:push": "prisma db push --schema=prisma/user.schema.prisma"
  },
  "keywords": ["express", "prisma", "mongodb", "jwt", "authentication"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.3.1",
    "@prisma/client": "^5.7.0",
    "express-validator": "^7.0.1",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "prisma": "^5.7.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "@types/jest": "^29.5.11"
  }
}
