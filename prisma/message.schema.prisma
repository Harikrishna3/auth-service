generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/@prisma/client-message"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Message {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  roomId    String
  senderId  String   @db.ObjectId
  message   String
  createdAt DateTime @default(now())

  @@map("messages")
}
